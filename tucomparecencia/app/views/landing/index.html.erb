<h1>Landing#index</h1>
<p>Find me in app/views/landing/index.html.erb</p>
<%= form_with(model: @region, local: true) do |form| %>
  <div class="field">
    <%= form.label :region_id %>
    <%= form.select :region_id, Region.pluck(:short_name, :id) %>

  </div>
  <div class="field">
    <%= form.label :commune_id %>
    <%= form.select :commune_id, Commune.pluck(:comuna, :id) %>

  </div>

<% end %>
